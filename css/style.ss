.orbit-container {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  z-index: 1;
  overflow: hidden;
}

.orbit-slides-container {
  width: 100%;
  height: 100% !important;
}
.orbit-slides-container li {
  width: 100%;
  height: 100%;
  background-size: cover;
}

